@model AcunMedyaAkademiPortfolio1.Models.TblAdress
@{
    ViewBag.Title = "CreateAdress";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Adres Bilgisi Ekleme </h2>
<form method="post">
    <label> Adres </label>
    @Html.TextBoxFor(x => x.Adress, new { @class = "form-control" })

    <label> Telefon Numarası </label>
    @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })

    <label> Mail Adresi </label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })

    <label> Websitesi</label>
    @Html.TextBoxFor(x => x.Website, new { @class = "form-control" })


    <button class=*@"btn btn-info"> Adres Bilgisini Kaydet </button>
</form>

